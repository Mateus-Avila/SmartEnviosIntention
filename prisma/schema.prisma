generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Intention {
  id              String @id @default(auto()) @map("_id") @db.ObjectId
  origin_cep      String
  destination_cep String
  lead            String?
}